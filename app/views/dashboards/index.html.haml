%div.container
  - @user_rows.each do |user_row|
    %div.row
      - user_row.each do |user|
        %div.span3
          %ul.thumbnails
            %li.span3
              %div.thumbnail
                %img{:src => user.profile_img_url, :alt => ""}
                %h5= user.name
                %p= user.current_task
